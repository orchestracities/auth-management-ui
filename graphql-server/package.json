{
  "name": "graphql-server",
  "version": "0.7.0",
  "keywords": [
    "graphql",
    "apollo",
    "keycloak",
    "authentication",
    "express"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "tsc --build",
    "watch": "tsc --build tsconfig.json --watch",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "test": "nyc ava -v",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "tslint '*/*/src/**/*.ts' --exclude 'src/**/*.test.ts' && tslint -c tslint_tests.json 'src/**/*.test.ts'",
    "release:prep": "./scripts/prepareRelease.sh",
    "release:validate": "./scripts/validateRelease.sh",
    "release:publish": "./scripts/publishRelease.sh",
    "examples:seed": "node scripts/initKeycloak.js"
  },
  "dependencies": {
    "@graphql-tools/utils": "7.2.3",
    "@types/express-session": "1.17.3",
    "@types/graphql": "14.2.3",
    "@types/keycloak-connect": "4.5.4",
    "@types/node": "10.17.50",
    "@types/sinon": "9.0.10",
    "apollo-server-express": "3.1.2",
    "cors": "2.8.5",
    "coveralls": "3.1.0",
    "dotenv": "^16.0.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "graphql": "15.4.0",
    "graphql-passport": "^0.6.4",
    "graphql-subscriptions": "1.1.0",
    "keycloak-admin": "1.14.4",
    "keycloak-connect": "12.0.1",
    "keycloak-connect-graphql": "^0.7.0",
    "keycloak-request-token": "0.1.0",
    "mongoose": "^6.2.11",
    "passport": "^0.6.0",
    "passport-keycloak-bearer": "^2.1.0",
    "passport-keycloak-oauth2-oidc": "^1.0.5",
    "passport-openid": "^0.4.0",
    "sinon": "9.2.2",
    "subscriptions-transport-ws": "0.9.18"
  }
}
